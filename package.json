{
  "name": "high-fives",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf public/css",
    "build": "npm run clean && parallelshell 'npm run styles' 'npm run assets'",
    "styles": "node-sass source/scss/ -o public/css",
    "assets": "ncp source/assets public/assets --stopOnErr",
    "prestart": "npm run build",
    "start": "parallelshell 'node tasks/watch.js' 'nodemon ./bin/www'"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "chokidar": "^1.7.0",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.9",
    "exit-hook": "^1.1.1",
    "express": "^4.15.5",
    "express-handlebars": "^3.0.0",
    "handlebars": "^4.0.11",
    "handlebars-helpers": "^0.9.8",
    "morgan": "~1.8.1",
    "ncp": "^2.0.0",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.6.2",
    "serve-favicon": "^2.4.5",
    "shelljs": "^0.7.8"
  }
}
